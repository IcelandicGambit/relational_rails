<header>
  <nav>
    <%= link_to "All Authors", "/authors" %>
    <%= link_to "All Books", "/books" %>
  </nav>
</header>

<h1>Authors</h1>
<h3><%=link_to 'New Author', '/authors/new'%>
<% @authors.each do |author| %>
  <h3><a href="/authors/<%= author.id %>"><%= author.name %></a></h3>
  <%= link_to 'Edit', "/authors/#{author.id}/edit"%>
<% end %> 