<%= form_with url: "/manufacturers", method: :post, local: true do |form| %>
<%=form.label :name%>
<%=form.text_field :name%>
<%=form.label :year_founded%>
<%=form.number_field :year_founded%>
<%=form.label :manufactured_in_us%>
<%= form.check_box :manufactured_in_us %>
<%= form.submit 'Create Manufacturer' %>
<% end %>

